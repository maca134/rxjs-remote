{
	"name": "@maca134/rxjs-remote",
	"version": "1.0.7",
	"description": "Observerables over the network",
	"main": "./dist",
	"scripts": {
		"update": "npm run update --prefix ../rxjs-remote-client && npm install @maca134/rxjs-remote-client@latest & git add ./* & git commit -m update & git push & npm version patch",
		"postversion": "npm run build && npm publish",
		"build": "rimraf ./dist && npm run generate-barrels && tsc",
		"generate-barrels": "barrelsby --delete -d ./src"
	},
	"author": "maca134",
	"license": "UNLICENSED",
	"dependencies": {
		"@maca134/rxjs-remote-client": "^1.0.7",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.5.3"
	},
	"devDependencies": {
		"@types/node": "^12.12.14",
		"barrelsby": "^2.2.0",
		"rimraf": "^3.0.0",
		"ts-node": "^8.5.2",
		"typescript": "^3.7.2"
	}
}
